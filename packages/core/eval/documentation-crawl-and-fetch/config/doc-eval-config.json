    {
      "id": "crossref-api",
      "name": "Crossref API",
      "description": "Scholarly metadata and DOI registration API",
      "documentationUrl": "https://api.crossref.org/swagger-ui/index.html",
      "urlHost": "https://api.crossref.org",
      "openApiUrl": "https://api.crossref.org/swagger-ui/index.html",
      "keywords": ["doi", "works", "publications", "journals", "members", "metadata", "citations"],
      "testQuestions": [
        "How do I search for works by DOI?",
        "What parameters can I use to filter journal metadata?"
      ]
    }
  ],
  "settings": {
    "crawlTimeout": 300000,
    "maxDocumentationSize": 500000,
    "enablePlaywrightCrawling": true,
    "enableOpenApiFetching": true,
    "enabledSites": ["openai-api"]
  }
}
