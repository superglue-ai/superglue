    },
    {
      "id": "thingsboard-api",
      "name": "ThingsBoard API",
      "description": "IoT platform API for device management and data collection",
      "documentationUrl": "https://demo.thingsboard.io/swagger-ui/index.html",
      "urlHost": "https://demo.thingsboard.io",
      "openApiUrl": "https://demo.thingsboard.io/swagger-ui/index.html",
      "keywords": ["devices", "telemetry", "attributes", "entities", "dashboards", "rules", "alarms"],
      "testQuestions": [
        "How do I send telemetry data from a device?",
