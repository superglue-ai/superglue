      "openApiUrl": "https://demo.caspio.com/integrations/rest/swagger/index.html",
      "keywords": ["tables", "records", "views", "applications", "authentication", "database"],
      "testQuestions": [
        "How do I retrieve records from a table?",
        "What authentication methods are supported?"
      ]
    },
    {
      "id": "epaypolicy-api",
